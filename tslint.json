{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "no-reference": false,
      "variable-name": [
        true,
        "ban-keywords",
        "check-format"
      ],
      "interface-name": {
        "options": [
          "never-prefix"
        ]
      },
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "member-ordering": false
    },
    "rulesDirectory": [],
    "linterOptions": {
      "exclude": [
        "dist/**/*",
        "node_modules/**/*",
        "bin/**/*",
        "package.json",
        "scripts/**/*"
      ]
    }
  }