{
  "name": "zamza",
  "version": "0.33.1",
  "description": "Apache Kafka discovery, indexing, searches, storage, hooks and HTTP gateway",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:nodefluent/zamza.git",
  "author": "Christian Froehlingsdorf <chris@5cf.de>",
  "license": "SEE LICENSE IN ./LICENSE",
  "private": false,
  "scripts": {
    "tsv": "tsc -v",
    "prepublishOnly": "yarn build",
    "build": "yarn lint && rm -rf dist/ && tsc -p tsconfig.json",
    "_start": "DEBUG=zamza:* node dist/example/index.js",
    "start": "yarn build && yarn _start",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "pkg:i": "./scripts/self-install.sh",
    "pkg:u": "./scripts/self-update.sh"
  },
  "keywords": [
    "apache",
    "kafka",
    "event",
    "streaming",
    "storage",
    "database",
    "metrics",
    "stats",
    "info",
    "information",
    "analysis",
    "rest",
    "http",
    "get",
    "call",
    "fetch",
    "zamza",
    "franz",
    "georg",
    "samsa",
    "nodefluent",
    "http",
    "gateway",
    "search",
    "index",
    "find",
    "seek"
  ],
  "dependencies": {
    "balrok": "^1.2.0",
    "bluebird": "~3.5.3",
    "body-parser": "~1.18.3",
    "commander": "~2.19.0",
    "cors": "~2.8.5",
    "debug": "~4.1.1",
    "express": "~4.16.4",
    "moment": "~2.24.0",
    "mongoose": "~5.4.7",
    "murmurhash": "~0.0.2",
    "prom-client": "~11.2.1",
    "ramda": "^0.26.1",
    "request": "~2.88.0",
    "sinek": "~6.24.0",
    "to-json-schema": "~0.2.0",
    "uuid": "~3.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "~3.5.25",
    "@types/cors": "~2.8.4",
    "@types/debug": "~0.0.31",
    "@types/express": "~4.16.1",
    "@types/mongoose": "~5.3.10",
    "@types/murmurhash": "~0.0.1",
    "@types/node": "~10.12.18",
    "@types/ramda": "^0.25.47",
    "@types/request": "~2.48.1",
    "@types/uuid": "~3.4.4",
    "mocha": "~5.2.0",
    "tslint": "~5.12.1",
    "typescript": "~3.2.4"
  },
  "preferGlobal": true,
  "bin": {
    "zamza": "bin/zamza.js"
  }
}
